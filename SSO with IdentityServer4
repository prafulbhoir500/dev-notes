        <span class="hljs-keyword">new</span> IdentityResource(<span class="hljs-string">"email"</span>, <span class="hljs-keyword">new</span>[] { <span class="hljs-string">"email"</span> }) <span class="hljs-comment">// Ensure email claim is included</span>
    };

