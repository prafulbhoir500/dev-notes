    <span class="hljs-keyword">new</span> List&lt;Client&gt;
    {
        <span class="hljs-keyword">new</span> Client
        {
            ClientId = <span class="hljs-string">"mvc"</span>,
            ClientSecrets = { <span class="hljs-keyword">new</span> Secret(<span class="hljs-string">"mvc-secret"</span>.Sha256()) },
            AllowedGrantTypes = GrantTypes.Code,
            RedirectUris = { <span class="hljs-string">"https://localhost:5002/signin-oidc"</span> },
