<span class="hljs-tag">&lt;<span class="hljs-name">asp:Button</span> <span class="hljs-attr">ID</span>=<span class="hljs-string">"btnLogout"</span> <span class="hljs-attr">runat</span>=<span class="hljs-string">"server"</span> <span class="hljs-attr">Text</span>=<span class="hljs-string">"Logout"</span> <span class="hljs-attr">OnClick</span>=<span class="hljs-string">"btnLogout_Click"</span> /&gt;</span>
