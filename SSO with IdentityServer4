builder.Services.AddAuthentication(options =&gt;
{
    options.DefaultScheme = <span class="hljs-string">"Cookies"</span>;
    options.DefaultChallengeScheme = <span class="hljs-string">"oidc"</span>;
})
.AddCookie(<span class="hljs-string">"Cookies"</span>)
.AddOpenIdConnect(<span class="hljs-string">"oidc"</span>, options =&gt;
{
    options.Authority = <span class="hljs-string">"https://localhost:5001"</span>;
    options.ClientId = <span class="hljs-string">"mvc"</span>;
    options.ClientSecret = <span class="hljs-string">"mvc-secret"</span>;
    options.ResponseType = <span class="hljs-string">"code"</span>;
    options.SaveTokens = <span class="hljs-literal">true</span>;
});

builder.Services.AddControllersWithViews();

<span class="hljs-keyword">var</span> app = builder.Build();

app.UseAuthentication();
app.UseAuthorization();
app.MapDefaultControllerRoute();
app.Run();
</span></code></div></div></pre><hr data-start="5057" data-end="5060"><h3 data-start="5062" data-end="5116"><strong data-start="5066" data-end="5116">2.3 Protect Secure Pages (<code data-start="5094" data-end="5113">HomeController.cs</code>)</strong></h3><pre class="!overflow-visible" data-start="5117" data-end="5339"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-[5px] h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">csharp</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none px-4 py-1" aria-label="Copy"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-xs"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy</button></span><span class="" data-state="closed"><button class="flex select-none items-center gap-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-xs"><path d="M2.5 5.5C4.3 5.2 5.2 4 5.5 2.5C5.8 4 6.7 5.2 8.5 5.5C6.7 5.8 5.8 7 5.5 8.5C5.2 7 4.3 5.8 2.