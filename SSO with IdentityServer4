<span class="hljs-keyword">using</span> Microsoft.Owin.Security.Cookies;
<span class="hljs-keyword">using</span> Microsoft.Owin.Security.OpenIdConnect;
<span class="hljs-keyword">using</span> Owin;
<span class="hljs-keyword">using</span> System.Threading.Tasks;

[<span class="hljs-meta">assembly: OwinStartup(typeof(WebFormsApp.Startup))</span>]
<span class="hljs-keyword">namespace</span> <span class="hljs-title">WebFormsApp</span>
{
    <span class="hljs-keyword">public</span> <span class="hljs-keyword">class</span> <span class="hljs-title">Startup</span>
    {
        <span class="hljs-function"><span class="hljs-keyword">public</span> <span class="hljs-keyword">void</span> <span class="hljs-title">Configuration</span>(<span class="hljs-params">IAppBuilder app</span>)</span>
        {
            app.UseCookieAuthentication(<span class="hljs-keyword">new</span> CookieAuthenticationOptions
            {
                AuthenticationType = <span class="hljs-string">"Cookies"</span>,
                LoginPath = <span class="hljs-keyword">new</span> PathString(<span class="hljs-string">"/Account/Login"</span>)
            });

            app.UseOpenIdConnectAuthentication(<span class="hljs-keyword">new</span> OpenIdConnectAuthenticationOptions
            {
                Authority = <span class="hljs-string">"https://localhost:5001"</span>,
                ClientId = <span class="hljs-string">"webforms"</span>,
                ClientSecret = <span class="hljs-string">"webforms-secret"</span>,
                ResponseType = <span class="hljs-string">"code"</span>,
                RedirectUri = <span class="hljs-string">"https://localhost:5003/signin-oidc"</span>,
                PostLogoutRedirectUri = <span class="hljs-string">"https://localhost:5003/signout-callback-oidc"</span>,
                SignInAsAuthenticationType = <span class="hljs-string">"Cookies"</span>,
                SaveTokens = <span class="hljs-literal">true</span>,
                Notifications = <span class="hljs-keyword">new</span> OpenIdConnectAuthenticationNotifications
                {
                    AuthenticationFailed = context =&gt;
                    {
                        context.HandleResponse();
                        context.Response.Redirect(<span class="hljs-string">"/Error"</span>);
                        <span class="hljs-keyword">return</span> Task.FromResult(<span class="hljs-number">0</span>);
                    }
                }
            });
        }
    }
}
</span></code></div></div></pre><hr data-start="5059" data-end="5062"><h2 data-start="5064" data-end="5113"><strong data-start="5067" data-end="5113">5. Implement Login and Logout in Web Forms</strong></h2><h3 data-start="5114" data-end="5157"><strong data-start="5118" data-end="5157">5.1 Create Login and Logout Buttons</strong></h3><p data-start="5158" data-end="5184">Update <strong data-start="5165" data-end="5183"><code data-start="5167" data-end="5181">Default.aspx</code></strong>:</p><pre class="!overflow-visible" data-start="5185" data-end="5363"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-[5px] h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">html</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none px-4 py-1" aria-label="Copy"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-xs"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy</button></span><span class="" data-state="closed"><button class="flex select